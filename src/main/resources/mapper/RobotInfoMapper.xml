<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxl.robot.dao.RobotInfoMapper">

	<!-- 通用查询结果列-->
	<sql id="Base_Column_List">
		id, robot_code, host,service_type,name, deploy,ranking, region, enabled, create_date,tenant_code
	</sql>

	<!--返回MAP -->
	<resultMap id="BaseResultMap" type="com.xxl.robot.entity.RobotInfo">
	<result column="id" property="id"/>
	<result column="robot_code" property="robotCode"/>
	<result column="host" property="host"/>
	<result column="service_type" property="serviceType"/>
	<result column="name" property="name"/>
	<result column="deploy" property="deploy"/>
	<result column="ranking" property="ranking"/>
	<result column="region" property="region"/>
	<result column="enabled" property="enabled"/>
	<result column="create_date" property="createDate"/>
	<result column="tenant_code" property="tenantCode"/>
	</resultMap>

</mapper>
