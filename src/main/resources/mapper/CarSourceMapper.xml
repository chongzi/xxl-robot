<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxl.robot.dao.CarSourceMapper">

	<!-- 通用查询结果列-->
	<sql id="Base_Column_List">
		id, sys_role_id, sys_resource_id
	</sql>

	<!--返回MAP -->
	<resultMap id="BaseResultMap" type="com.xxl.robot.entity.CarSource">
	<result column="id" property="id"/>
	<result column="sys_role_id" property="sysRoleId"/>
	<result column="sys_resource_id" property="sysResourceId"/>
	</resultMap>

</mapper>
