<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxl.robot.dao.CarQqMapper">

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		id, session_time, content
	</sql>

	<!--返回MAP -->
	<resultMap id="BaseResultMap" type="com.xxl.robot.entity.CarQq">
		<result column="id" property="id" />
		<result column="session_time" property="sessionTime" />
		<result column="content" property="content" />
	</resultMap>

	<insert id="insertBatch" parameterType="java.util.List">
		insert into car_qq(session_time, content)
		values
		<foreach collection="list" item="item" index="index" separator=",">
			(now(), #{item.content,jdbcType=VARCHAR})
		</foreach>
	</insert>
	
	
</mapper>
